VITE_SUPABASE_URL=https://your-project-ref.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Obtén esto desde Supabase Dashboard > Settings > Database > Connection Pooling
# Formato: postgresql://postgres.[PROJECT-REF]:[PASSWORD]@aws-0-[REGION].pooler.supabase.com:6543/postgres?pgbouncer=true
DATABASE_URL=postgresql://postgres.your-project-ref:your-password@aws-0-us-east-1.pooler.supabase.com:6543/postgres?pgbouncer=true&connection_limit=1

NODE_ENV=development
PORT=3001

# IMPORTANTE: Cambia estos secrets en producción
JWT_SECRET=change_this_to_a_secure_random_string_in_production
JWT_REFRESH_SECRET=change_this_to_another_secure_random_string_in_production
